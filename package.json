{
  "name": "cwr-parser",
  "version": "2.0.3",
  "description": "CWR v21/v22 file parser with OOP classes",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.d.ts"
    },
    "./node": {
      "import": "./dist/node/CWRParserNode.js",
      "types": "./dist/node/CWRParserNode.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "jest": "^30.0.3",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zappbrandigan/cwr-parser.git"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0"
}
